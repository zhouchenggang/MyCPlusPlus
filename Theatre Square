Reference : https://codeforces.com/problemset/problem/1/A

//这是纯数学解法，重点在于cmath函数的灵活运用。

#include <iostream>
#include <cmath>

using namespace std;

int main() {
    int n, m, a;
    cin >> n >> m >> a;

    int num_flagstones = ceil((double)n/a) * ceil((double)m/a);

    cout << num_flagstones << endl;

    return 0;
}
